{
  "name": "Astrogon",
  "version": "1.0.0",
  "description": "Template.",
  "author": "Frickeldave",
  "license": "MIT",
  "packageManager": "npm@10.8.2",
  "engines": {
    "node": ">=24.0.0"
  },
  "scripts": {
    "dev": "npm run generate-version && astro dev",
    "build": "astro build",
    "prebuild": "npm run generate-version",
    "postbuild": "pagefind --site dist",
    "generate-version": "tsx scripts/generate-version.ts",
    "prepare": "husky install",
    "postinstall": "npm run install-vale",
    "format": "prettier --write ./src --log-level silent",
    "format:check": "prettier --check ./src",
    "lint": "eslint ./src --fix",
    "lint:check": "eslint ./src",
    "install-vale": "node -e \"const cmd = process.platform === 'win32' ? 'powershell -ExecutionPolicy Bypass -File .vale/install-vale.ps1' : 'bash .vale/install-vale.sh'; require('child_process').exec(cmd, (err, stdout, stderr) => { if (err) { console.error(stderr); process.exit(1); } console.log(stdout); })\"",
    "prose": "node -e \"const { spawn } = require('child_process'); const path = require('path'); const cmd = process.platform === 'win32' ? path.join('.vale', 'vale.exe') : path.join('.vale', 'vale'); const proc = spawn(cmd, ['--config=.vale.ini', 'src/content'], { stdio: 'inherit' }); proc.on('close', (code) => process.exit(code));\""
  },
  "dependencies": {
    "@astrojs/cloudflare": "^12.6.4",
    "@astrojs/mdx": "^4.3.4",
    "@astrojs/react": "^4.3.0",
    "@astrojs/rss": "^4.0.12",
    "@astrojs/sitemap": "^3.5.0",
    "@astrojs/tailwind": "^6.0.2",
    "@radix-ui/react-scroll-area": "^1.1.0",
    "astro": "^5.13.2",
    "astro-auto-import": "^0.4.2",
    "astro-tooltips": "^0.6.2",
    "clsx": "^2.1.1",
    "date-fns": "^2.30.0",
    "date-fns-tz": "^2.0.0",
    "fuse.js": "^6.6.2",
    "github-slugger": "^2.0.0",
    "gray-matter": "^4.0.3",
    "katex": "^0.16.21",
    "marked": "^6.0.0",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^5.5.0",
    "react-lite-youtube-embed": "^2.3.52",
    "rehype-katex": "^7.0.1",
    "remark-collapse": "^0.1.2",
    "remark-math": "^6.0.0",
    "remark-toc": "^8.0.1",
    "sharp": "^0.32.4",
    "swiper": "^10.1.0",
    "tailwind-merge": "^2.5.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@tailwindcss/forms": "^0.5.4",
    "@tailwindcss/typography": "^0.5.9",
    "@types/marked": "^5.0.1",
    "@types/node": "^24.9.2",
    "@types/react": "18.2.18",
    "@types/react-dom": "18.2.7",
    "@typescript-eslint/eslint-plugin": "^8.52.0",
    "@typescript-eslint/parser": "^8.52.0",
    "astro-eslint-parser": "^1.2.2",
    "autoprefixer": "^10.4.14",
    "eslint": "^9.39.2",
    "eslint-plugin-astro": "^1.5.0",
    "globals": "^17.0.0",
    "husky": "^8.0.3",
    "pagefind": "^1.0.4",
    "postcss": "^8.4.27",
    "prettier": "^3.7.4",
    "prettier-plugin-astro": "^0.14.1",
    "prettier-plugin-tailwindcss": "^0.7.2",
    "sass": "^1.64.2",
    "tailwind-bootstrap-grid": "^5.0.1",
    "tailwindcss": "^3.3.3",
    "tsx": "^4.20.6",
    "typescript": "5.1.6",
    "typescript-eslint": "^8.52.0",
    "wrangler": "^4.24.0"
  }
}
